class Solution:
    def jump(self, nums: List[int]) -> int:
        #TimeComplexity:O(n);
        #SpaceComplexity:O(1)
        if len(nums) == 1:
            return 0
    
        l, r = 1,nums[0]+1
    
        farthest = r - 1
    
        jumps = 1
    
        while farthest < len(nums) - 1:
            for i in range(l, r):
                farthest = max(farthest, i + nums[i] )

            l = r
            r = farthest+1
            jumps += 1
    
        return jumps
